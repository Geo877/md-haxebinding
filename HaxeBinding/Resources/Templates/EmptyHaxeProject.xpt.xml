<?xml version="1.0"?>
<Template Originator   = "Federico Di Gregorio"
          Created      = "30/04/2009"
          LastModified = "30/04/2009">
        
    <TemplateConfiguration>
        <_Name>Empty Haxe Project</_Name>
        <_Category>Haxe</_Category>
        <Icon>fd-project</Icon>
        <LanguageName>Haxe</LanguageName>
        <_Description>Creates an empty Haxe project</_Description>
    </TemplateConfiguration>

    <Combine name="${ProjectName}" directory=".">
        <Project name="${ProjectName}" directory="." type="Haxe">
            <Options TargetFormat="SWF" MainLanguage="Haxe"/>
            <Files>
                <FileTemplateReference TemplateID="EmptyHaxeSourceFile" name="${ProjectName}.hx" />
            </Files>
        </Project>
    </Combine>
</Template>
